					</template>
				</el-table-column>
				<el-table-column prop="dwyc" label="定位异常" width="180">
					<template slot-scope="scope">
						<table-column-malfunction :item="scope.row" :fault="scope.row.dwyc" :system-fault="scope.row.dwycxt" type="定位异常" @true-review-click="" @false-review-click=""></table-column-malfunction>
					</template>
				</el-table-column>
				<el-table-column prop="kcyc" label="空车异常" width="180">
					<template slot-scope="scope">
						<table-column-malfunction :item="scope.row" :fault="scope.row.kcyc" :system-fault="scope.row.kcycxt" type="空车异常" @true-review-click="" @false-review-click=""></table-column-malfunction>
					</template>
				</el-table-column>
				<el-table-column prop="jjbjyc" label="紧急报警异常" width="180">
					<template slot-scope="scope">
						<table-column-malfunction :item="scope.row" :fault="scope.row.jjbjyc" :system-fault="scope.row.jjbjycxt" type="紧急报警异常" @true-review-click="" @false-review-click=""></table-column-malfunction>
